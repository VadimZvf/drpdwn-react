{
  "name": "drpdwn-react",
  "version": "1.0.0",
  "description": "react dropdown",
  "main": "index.js",
  "scripts": {
    "compile": "npm run clean && npm run compile:lib && npm run compile:flow",
    "compile:lib": "babel src --out-dir lib --ignore \"src/**/__tests__\",\"src/**/stories.js\" --copy-files",
