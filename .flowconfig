[ignore]
.*/node_modules/.*

[include]


[libs]
flow
flow-typed

[options]
esproposal.class_instance_fields=enable
esproposal.class_static_fields=enable
module.name_mapper.extension='css' -> '<PROJECT_ROOT>/flow-mock/CSSModule.js.flow'
module.name_mapper.extension='png' -> '<PROJECT_ROOT>/flow-mock/WebpackAsset.js.flow'
module.name_mapper.extension='jpg' -> '<PROJECT_ROOT>/flow-mock/WebpackAsset.js.flow'
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
module.system=haste
